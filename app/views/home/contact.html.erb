<%= form_with model: @message do |f| %>
<%= f.label :name, "Name" %>
<%= f.text_field :name %>
<br>
<%= f.label :email, "Email" %>
<%= f.email_field :email %>
<br>
<%= f.label :phone_number, "Phone Number" %>
<%= f.text_field :phone_number %>
<br>
<%= f.label :body, "Body" %>
<%= f.text_area :body %>
<br>
<%= f.submit %>
<% end %>
